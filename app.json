{
  "expo": {
    "name": "google-login-app",
    "slug": "google-login-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "googleloginapp",
    "userInterfaceStyle": "automatic",

    "android": {
      "package": "com.abdullahdfs.myapp"
    },

    "extra": {
      "eas": {
        "projectId": "1bc01c4a-0d43-4d7c-ba9e-ce3733d3ae55"
      }
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ]
  }
}
